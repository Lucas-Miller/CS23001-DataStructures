#include "stack.hpp"
#include <cassert>
#include <iostream>

int main() {
    
    {
        stack test();
        test.push(1);
        
        assert(test.empty() == false);
        assert(test.top() == 1);
    }
    
    
        
    {
        stack test();
        test.push(1);
        test.pop();
        
        assert(test.empty() == true);
    }
    
}
